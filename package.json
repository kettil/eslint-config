{
  "name": "@kettil/eslint-config",
  "version": "3.5.0",
  "description": "The library is a collection of very strict ESLint rules",
  "main": "node.js",
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "test": "npx carna test",
    "prepare": "[ \"$CI\" != \"\" ] || husky install ./node_modules/carna/configs/husky",
    "analyse": "npx carna analyse",
    "deps": "npx carna deps",
    "license": "npx carna license",
    "build": "npx carna build",
    "prebuild": "rm -rf ./build",
    "test:unit": "npx carna test -p unit -w"
  },
  "keywords": [
    "ecmascript",
    "es6",
    "es2015",
    "es2016",
    "es2017",
    "es2018",
    "es2019",
    "es2020",
    "eslint",
    "eslint-config",
    "jest",
    "react",
    "typescript"
  ],
  "author": "Kjell Die√üel <development@kettil.de>",
  "license": "MIT",
  "engines": {
    "node": ">=14",
    "npm": ">=6"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/kettil/eslint-config"
  },
  "bugs": {
    "url": "https://github.com/kettil/eslint-config/issues/new"
  },
  "dependencies": {
    "@typescript-eslint/eslint-plugin": "4.31.2",
    "@typescript-eslint/parser": "4.31.2",
    "eslint-plugin-eslint-comments": "3.2.0",
    "eslint-plugin-formatjs": "2.17.6",
    "eslint-plugin-import": "2.24.2",
    "eslint-plugin-jest": "24.4.2",
    "eslint-plugin-jest-dom": "3.9.2",
    "eslint-plugin-jsx-a11y": "6.4.1",
    "eslint-plugin-node": "11.1.0",
    "eslint-plugin-react": "7.26.0",
    "eslint-plugin-react-hooks": "4.2.0",
    "eslint-plugin-regexp": "1.1.0",
    "eslint-plugin-sonarjs": "0.10.0",
    "eslint-plugin-testing-library": "4.12.3",
    "eslint-plugin-unicorn": "34.0.1"
  },
  "devDependencies": {
    "carna": "4.10.6",
    "typescript": "4.4.3"
  }
}
